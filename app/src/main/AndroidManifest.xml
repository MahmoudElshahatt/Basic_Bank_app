<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.basicbankappfversion">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="false"
        android:theme="@style/Theme.BasicBankAppFversion">
        <activity
            android:name=".UI.SendToUserActivity"
            android:exported="false"
            android:label="Send To a User"/>
        <activity
            android:name=".UI.UserDataActivity"
            android:exported="false"
            android:label="@string/userData"
            android:parentActivityName=".UI.UsersActivity" />
        <activity
            android:name=".UI.TransfersActivity"
            android:exported="false"
            android:label="@string/transfers"
            android:parentActivityName=".UI.HomeActivity" />
        <activity
            android:name=".UI.UsersActivity"
            android:exported="false"
            android:label="@string/users"
            android:parentActivityName=".UI.HomeActivity" />
        <activity
            android:name=".UI.HomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>